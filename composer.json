{
    "name": "innoscripta/sipgate",
    "description": "PHP library for integrating with sipgate",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "blastcloud/guzzler": "^1.5",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Innoscripta\\Sipgate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Innoscripta\\Sipgate\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
