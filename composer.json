{
    "name": "orkhanahmadov/sipgate",
    "description": "PHP library for integrating with sipgate",
    "keywords": [
        "sip",
        "sipgate"
    ],
    "homepage": "https://github.com/orkhanahmadov/sipgate",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Orkhan Ahmadov",
            "email": "ahmadov90@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "guzzlehttp/guzzle": "^6.3",
        "ext-json": "*"
    },
    "require-dev": {
        "blastcloud/guzzler": "^1.5",
        "phpunit/phpunit": "^6.0|^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Orkhanahmadov\\Sipgate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Orkhanahmadov\\Sipgate\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    }
}
